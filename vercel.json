{
 "version": 2,
 "builds": [
  {
   "src": "*.ts",
   "use": "@vercel/node"
  }
 ],
 "crons": [
  {
   "path": "/api/building/route",
   "schedule": "0 0 * * *"
  }
 ],
 "cleanUrls": true,
 "rewrites": [
  {
   "source": "/api/(.*)",
   "destination": "/api/$1"
  },
  {
   "source": "/(.*)",
   "destination": "/"
  }
 ]
}
